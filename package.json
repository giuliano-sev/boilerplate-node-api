{
  "name": "boilerplate-node-api",
  "version": "1.0.0",
  "description": "This boilerplate uses NodeJs with Express as API Framework, JWT as authentication and Objection as ORM. All wrapped with Typescript support",
  "main": "app.js",
  "scripts": {
    "start": "tsc && node app.js",
    "build": "tsc",
    "sequelize": "dotenv sequelize -- ",
    "pretty": "prettier --write \"./src/**/*.ts\"",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giuliano-sev/boilerplate-node-api.git"
  },
  "author": "github/giuliano-sev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/giuliano-sev/boilerplate-node-api/issues"
  },
  "homepage": "https://github.com/giuliano-sev/boilerplate-node-api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mysql2": "^2.0.2",
    "node-password-util": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.3",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^8.3.2",
    "dotenv-cli": "^2.0.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "git+https://github.com/giulianoSev/prettier.git",
    "sequelize-cli": "^5.5.1",
    "typescript": "^3.7.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
