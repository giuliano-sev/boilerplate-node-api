  "bugs": {
    "url": "https://github.com/giuliano-sev/boilerplate-node-api/issues"
  },
  "homepage": "https://github.com/giuliano-sev/boilerplate-node-api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "node-password-util": "^1.0.1",
    "objection": "^1.6.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
